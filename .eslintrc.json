{
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "react/jsx-indent": 1,
    "react/no-danger": 0,
    "react/prop-types": 0,
    "no-unused-vars": 1,
    "no-trailing-spaces": 1,
    "react/jsx-tag-spacing": 1
  }
}
